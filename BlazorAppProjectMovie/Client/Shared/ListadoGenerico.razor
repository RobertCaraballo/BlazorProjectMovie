@typeparam Titem

@if (Listado == null)
{
    @if (Cargando == null)
    {
        <text>Buscando peliculavghvhs</text>
    }
    else
    {
        @Cargando
    }

}
else if (Listado.Count == 0)
{
    @if (NoHayRegistros == null)
    {
        <text>No hay registros para mostrar</text>
    }
    else
    {
        @NoHayRegistros
    }
}

else
{
    // ****** Aqui hay un error, No carga la vista de las peliculas, tan solo el @HayRegistrosCompeto ********** //
    if (HayRegistros != null)
    {

        foreach (var item in Listado)
        {
            @thereAreDate(item)
            ;
        }

    }
    else
    {
        @HayRegistrosCompeto
        ;
    }

    @*    @foreach (var pelicula in peliculas)
        {
        <PeliculaIndividual Pelicula="pelicula" EliminarPelicula="Eliminar" />
        }*@

}

@code {

    [Parameter] public RenderFragment Cargando { get; set; }
    [Parameter] public RenderFragment<Titem> thereAreDate { get; set; }
    [Parameter] public RenderFragment HayRegistrosCompeto { get; set; }
    [Parameter] public RenderFragment HayRegistros { get; set; }
    [Parameter] public RenderFragment NoHayRegistros { get; set; }

    [Parameter] public List<Titem> Listado { get; set; }


}